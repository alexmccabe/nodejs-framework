{
  "app": {
    "allowedOrigins": [],
    "paths": {
      "masterScript": "/app/modules/onLaunch.js",
      "threadScript": "/app/modules/onThreadStart.js",
      "logDir": "../logs",
      "staticAssetDir": "../public",
      "templateDir": "../templates",
      "tmpDir": "../tmp"
    }
  },

  "bodyParser": {
    "json": {},
    "urlencoded": {
      "extended": true
    }
  },

  "cdn": {
    "baseUrl": "",
    "provider": "",
    "config": {
      "aws": {
        "endpoint": "s3.amazonaws.com",
        "region": "eu-west-1",
        "usePromise": true
      },

      "s3": {
        "Bucket": ""
      }
    }
  },

  "mongoose": {
    "useNewUrlParser": true
  },

  "ratelimit": {
    "api": {
      "max": 2500,
      "duration": 3600000,
      "flood": 5
    }
  },

  "security": {
    "helmetCSP": {
      "directives": {
        "defaultSrc": ["'self'"]
      },

      "browserSniff": true,
      "disableAndroid": false,
      "loose": false,
      "reportOnly": false,
      "setAllHeaders": false
    },

    "referrerPolicy": ""
  }
}
