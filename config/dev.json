{
  "app": {
    "allowedOrigins": ["http://localhost:3000", "http://localhost:5000"],
    "paths": {
      "masterScript": "/modules/onLaunch.js",
      "threadScript": "/modules/onThreadStart.js",
      "logDir": "/logs"
    }
  },

  "bodyParser": {
    "json": {},
    "urlencoded": {
      "extended": true
    }
  },

  "cdn": {
    "baseUrl": "",
    "provider": "",
    "config": {
      "s3": {
        "defaultBucket": "",
        "endpoint": "s3.amazonaws.com",
        "region": "eu-west-1"
      }
    }
  },

  "mongoDB": {},

  "ratelimit": {
    "api": {
      "max": 2500,
      "duration": 3600000,
      "flood": 5
    }
  },

  "security": {
    "helmetCSP": {
      "directives": {
        "defaultSrc": ["'self'"]
      },

      "browserSniff": true,
      "disableAndroid": false,
      "loose": false,
      "reportOnly": false,
      "setAllHeaders": false
    },

    "referrerPolicy": ""
  }
}
